    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.3",
    "mocha": "^10.7.3",
    "nyc": "^17.0.0",
    "sinon": "^18.0.0"
  },
  "files": [
    "lib/"
  ],
